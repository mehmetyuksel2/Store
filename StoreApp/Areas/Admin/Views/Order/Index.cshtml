@model IEnumerable<Order>

@if(Model.Count() > 0)
{
    @foreach(Order order in Model)//tüm orderlar burada listeleniyor
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@order" /><!--her bir order model olarak orderDetailse gönderiliyor-->
        
        </div>
    }
}
else
{
    <div class="alert alert-info">
        <p class="lead">There is no order yet.</p>
    </div>
}